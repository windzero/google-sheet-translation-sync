  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "env-cmd": "^10.0.1",
    "google-sheet-translation-sync": "^2.0.2",
    "husky": "^4.0.10",
    "nodemon": "^2.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "googleapis": "39",
